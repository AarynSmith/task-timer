{
  "name": "task-timer",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "electron-start": "yarn expo-electron start",
    "electron-build": "yarn electron-webpack && yarn electron-builder --dir -c.compression=store -c.mac.identity=null",
    "electron-build-prod": "yarn electron-webpack && yarn electron-builder -w -m"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "expo": "~46.0.9",
    "expo-status-bar": "~1.4.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.5",
    "react-native-web": "~0.18.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/electron-adapter": "^0.0.55",
    "@expo/webpack-config": "^0.17.2",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.69.1",
    "@types/uuid": "^8.3.4",
    "electron": "^6.0.12",
    "electron-builder": "^23.3.3",
    "typescript": "~4.3.5"
  },
  "private": true,
  "build": {
    "extraMetadata": {
      "main": "main.js"
    },
    "files": [
      {
        "from": "dist/main/",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist/renderer",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      "package.json",
      "**/node_modules/**/*"
    ]
  }
}
